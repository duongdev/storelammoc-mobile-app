      "config": {
        "branch": {
          "apiKey": "key_live_eiUgn4Z18J4wQ08JIMCkTbjgFrnOfNXH"
        }
      }
    },
    "android": {
      "package": "com.lammoc.store",
      "config": {
        "branch": {
          "apiKey": "key_live_eiUgn4Z18J4wQ08JIMCkTbjgFrnOfNXH"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
