{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "assets": "./assets",
          "screens": "./src/screens",
          "components": "./src/components",
          "env": "./src/env",
          "constants": "./src/constants",
          "hocs": "./src/hocs",
          "utils": "./src/utils",
          "helpers": "./src/helpers",
          "navigations": "./src/navigations"
        }
      }
    ]
  ]
}
