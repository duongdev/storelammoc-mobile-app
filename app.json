{
  "expo": {
    "name": "Store Làm Mộc",
    "description": "Hệ thống siêu thị chuyên bán máy móc và dụng cụ làm mộc.",
    "slug": "store-lam-moc",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.0",
    "primaryColor": "#263238",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#263238"
    },
    "notification": {
      "icon": "./assets/images/logo.png",
      "color": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidStatusBar": {
      "backgroundColor": "#26323800"
    },
    "ios": {
      "buildNumber": "1",
      "supportsTablet": false,
      "bundleIdentifier": "com.lammoc.store"
    },
    "android": {
      "package": "com.lammoc.store",
      "versionCode": 1
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4",
        "png"
      ],
      "sourceExts": [
        "ts",
        "tsx"
      ],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "./notify",
          "config": {
            "webhookUrl": "https://hooks.slack.com/put-your-stuff-here"
          }
        }
      ]
    }
  }
}
