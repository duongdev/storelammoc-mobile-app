{
  "expo": {
    "name": "Store Làm Mộc Staging",
    "description": "Hệ thống siêu thị chuyên bán máy móc và dụng cụ làm mộc.",
    "slug": "store-lam-moc-staging",
    "privacy": "unlisted",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.1.0",
    "primaryColor": "#263238",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#263238"
    },
    "notification": {
      "icon": "./assets/images/logo.png",
      "color": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidStatusBar": {
      "backgroundColor": "#26323800"
    },
    "ios": {
      "buildNumber": "2",
      "supportsTablet": false,
      "bundleIdentifier": "com.lammoc.store.staging",
      "icon": "./assets/images/logo.ios.png",
      "infoPlist": {
        "NSCameraUsageDescription": "Cho phép Store Làm Mộc sử dụng máy ảnh để quét mã vạch/mã QR"
      }
    },
    "android": {
      "package": "com.lammoc.store.staging",
      "versionCode": 2,
      "icon": "./assets/images/logo.android.png",
      "adaptiveIcon": {
        "backgroundColor": "#263238"
      },
      "permissions": [
        "CAMERA"
      ]
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4",
        "png"
      ],
      "sourceExts": [
        "ts",
        "tsx"
      ],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    },
    "extra": {
      "STORE_WEB_URL": "https://store.mock.love",
      "API_URL": "https://api.mock.love"
    }
  }
}